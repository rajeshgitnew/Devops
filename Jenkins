def MyName = "Rajesh"

pipeline {
agent any 
stages {
stage ("print variable") {
steps { 
echo "My Name is ${MyName}"
    }
   }
stage ("compile") {
steps {
//git url: 'https://github.com/rajeshgitnew/java.git', branch: 'master'
sh "mvn -DskipTests clean  install"
   }
  post {
        success {
                sh "mvn deploy"
                }    
         }
         }
    }
 }   
